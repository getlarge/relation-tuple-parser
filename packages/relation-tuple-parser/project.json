{
	"name": "relation-tuple-parser",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "packages/relation-tuple-parser/src",
	"projectType": "library",
	"targets": {
		"build": {
			"executor": "@nrwl/rollup:rollup",
			"outputs": ["{options.outputPath}"],
			"options": {
				"outputPath": "dist/packages/relation-tuple-parser",
				"main": "packages/relation-tuple-parser/src/index.ts",
				"tsConfig": "packages/relation-tuple-parser/tsconfig.lib.json",
				"assets": []
			}
		},
		"publish": {
			"executor": "nx:run-commands",
			"options": {
				"command": "node tools/scripts/publish.mjs relation-tuple-parser {args.ver} {args.tag}"
			},
			"dependsOn": ["build"]
		},
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": ["packages/relation-tuple-parser/**/*.ts"]
			}
		},
		"test": {
			"executor": "@nrwl/jest:jest",
			"outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
			"options": {
				"jestConfig": "packages/relation-tuple-parser/jest.config.ts",
				"passWithNoTests": true
			}
		}
	},
	"tags": []
}
